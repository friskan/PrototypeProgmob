<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeAdminActivity">

    <ImageButton
        android:id="@+id/imgButtonDataDiri"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/flower1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Data Diri"
        app:layout_constraintEnd_toEndOf="@+id/imgButtonDataDiri"
        app:layout_constraintStart_toStartOf="@+id/imgButtonDataDiri"
        app:layout_constraintTop_toBottomOf="@+id/imgButtonDataDiri" />

    <ImageButton
        android:id="@+id/imgButtonKelolaKrs"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="172dp"
        android:src="@drawable/flower4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Daftar Dosen"
        app:layout_constraintEnd_toEndOf="@+id/imgButtonDaftarDosen"
        app:layout_constraintHorizontal_bias="0.527"
        app:layout_constraintStart_toStartOf="@+id/imgButtonDaftarDosen"
        app:layout_constraintTop_toBottomOf="@+id/imgButtonDaftarDosen" />

    <ImageButton
        android:id="@+id/imgButtonDaftarDosen"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/flower2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Daftar Matkul"
        app:layout_constraintEnd_toEndOf="@+id/imgButtonDaftarMatkul"
        app:layout_constraintStart_toStartOf="@+id/imgButtonDaftarMatkul"
        app:layout_constraintTop_toBottomOf="@+id/imgButtonDaftarMatkul" />

    <ImageButton
        android:id="@+id/imgButtonDaftarMhs"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="172dp"
        android:src="@drawable/flower5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Kelola KRS"
        app:layout_constraintEnd_toEndOf="@+id/imgButtonKelolaKrs"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="@+id/imgButtonKelolaKrs"
        app:layout_constraintTop_toBottomOf="@+id/imgButtonKelolaKrs" />

    <ImageButton
        android:id="@+id/imgButtonDaftarMatkul"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/flower3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Daftar Mahasiswa"
        app:layout_constraintEnd_toEndOf="@+id/imgButtonDaftarMhs"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="@+id/imgButtonDaftarMhs"
        app:layout_constraintTop_toBottomOf="@+id/imgButtonDaftarMhs" />

</androidx.constraintlayout.widget.ConstraintLayout>